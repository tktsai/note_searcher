{
	"name": "note-searcher",
	"displayName": "note-searcher",
	"description": "searches notes",
	"version": "0.0.1",
	"publisher": "uozuaho",
	"repository": {
		"type": "git",
		"url": "https://github.com/uozuAho/note_searcher"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.search",
		"onCommand:extension.index",
		"onView:noteSearcher"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "noteSearcher",
					"title": "Note Searcher",
					"icon": "img/spiral.png"
				}
			]
		},
		"views": {
			"noteSearcher": [
				{
					"id": "noteSearcher-results",
					"name": "Results"
				}
			]
		},
		"commands": [
			{
				"command": "extension.search",
				"title": "Note searcher: search for docs"
			},
			{
				"command": "extension.index",
				"title": "Note searcher: index docs in the current workspace"
			}
		],
		"keybindings": [
			{
				"command": "extension.search",
				"key": "ctrl+alt+s"
			},
			{
				"command": "extension.index",
				"key": "ctrl+alt+i"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
